<launch>

    <include file="$(find multimaster_launch)/launch/multimaster_gazebo_robot.launch">
        <!-- Alt yürür için gerken multimaster node-->
        <arg name="robot_namespace" value="rover_21_description" />
    </include>

    <!-- launch custom world if you want custom world un comment that line-->
    <include file="$(find marsyard)/launch/arm_world.launch" />

    <!-- <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="paused" value="true" />
    </include> -->

    <param name="robot_description" command="xacro --inorder $(find arm_22_gazebo_vel)/urdf/assembly.urdf.xacro" />


    <node name="spawn_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model arm_22_gazebo_vel" output="screen" />
    <!-- robot state publisher-->
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

    <include file="$(find arm_control)/launch/arm_22_controllers_vel.launch" />
</launch>